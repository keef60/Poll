{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source":"/orders/Customers",
        "function": "app"

      },
      {
        "source":"/customer/Messages",
        "function": "app"

      }
    ]
  }
}
